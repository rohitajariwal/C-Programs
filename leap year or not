#include <stdio.h>

int main()
{
    int year;
        printf("Enter any year value: ");
        scanf("%d",&year);
        if ( ( (year%4 == 0) && (year%100 !=0) ) || (year%400 == 0))
        printf("This year is definitely a leap year");
        else
        printf("This year is not at all leap year");
    return 0;
}
