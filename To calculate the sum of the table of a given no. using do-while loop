#include <stdio.h>

int main()
{
    int num, i=1, t, table, sum;
    
    printf("Enter the number\n");
    scanf("%d",&num);
    printf("How many times\n");
    scanf("%d",&t);
    
    do
    {
        table = num * i;
        sum = table;
        table = sum + table;
        i++;
    }
    while (i<=t);
    
    printf("The sum of multiplication is %d",sum);
        
    return 0;
}
