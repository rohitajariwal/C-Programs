#include <stdio.h>

int main()
{
    int a,b;
    
    printf("Enter the first number");
    scanf("%d",&a);
    printf("Enter the second number\n");
    scanf("%d",&b);
    if(a>b)
    printf("%d is the greatest number\n",a);
    else
    printf("%d is the greatest number\n",b);
    printf("\nEnd of program");

    return 0;
}
