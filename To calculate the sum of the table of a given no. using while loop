#include <stdio.h>

int main()
{
    int num, i=1 , t, table, sum;
    
    printf("Enter the value\n");
    scanf("%d",&num);
    printf("How many times\n");
    scanf("%d",&t);
    
    while(i<=t)
    {
        table = num * i;
        sum = table;
        sum = table + sum;
        i++;
    }
    printf("The sum of the multiplication is %d",sum);

    return 0;
}
