#include <stdio.h>

int main()
{
    char ch;
    printf("Enter a Character\n");
    scanf("%c",&ch);
    
    if(ch>='a' && ch<='z')
        {
        printf("Lowercase Character\n");
        }
    else if(ch>='A' && ch<='Z')
        {
        printf("Uppercase Character\n");
        }
    else if(ch>='0' && ch<='9')
        {
        printf("Digit Character");
        }
    else 
        {
        printf("Special Character");
        }
    return 0;
}
